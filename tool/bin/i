#!/usr/bin/env node

const { exec } = require('child_process');
exec('tnpm install', (err, stdout, stderr) => {
  if (err) {
    exec('npm install', (err, stdout, stderr) => {
      if (err) {
        console.error(err);
        return;
      }
      console.log(stdout);
    });
    return;
  }
  console.log(stdout);
});

