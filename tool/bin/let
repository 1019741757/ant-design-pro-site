#!/usr/bin/env node

require('iconv-lite').extendNodeEncodings();

var pkg = require('../package');
var program = require('commander');

var move = require('../lib/move');

program.version('0.1.0')
    .option('-m, --move', 'move scaffold to bisheng _site')
    .parse(process.argv);

if ( program.move ) {
  move(program.move);
}
